
@{
    ViewData["Title"] = "NotFound";
}

<!-- Error Section -->
<section class="error-section">
    <div class="pattern-layer"
         style="background-image: url(./Home/images/background/pattern-8.png)"></div>
    <div class="auto-container">
        <div class="row clearfix">
            <!-- Content Column -->
            <div class="content-column col-lg-6 col-md-12 col-sm-12">
                <div class="inner-column">
                    <h1>Page Not Found</h1>
                    <div class="text">
                        Lorem Ipsum is simply dummy text of the printing and
                        typesetting Ipsum has been the industry's standard dummy text
                        ever since.
                    </div>
                    <a asp-controller="Home" asp-action="Index" class="theme-btn btn-style-one"
                            type="submit"
                            name="submit-form">
                        <span class="txt">
                            go to home <i class="fas fa-chevron-circle-right"></i>
                        </span>
                    </a>
                </div>
            </div>

            <!-- Image Column -->
            <div class="image-column col-lg-6 col-md-12 col-sm-12">
                <div class="inner-column">
                    <div class="image">
                        <img src="~/Home/images/carImages/not-found.png" alt="" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Error Section -->
